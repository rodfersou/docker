bind 0 select 10

bind h prev
bind l next

hardstatus alwayslastline
hardstatus string "%{gk}[%{g}%H%{g}][%{wk}%-w%{=b kR}|%{=d}%{W}%n%f%{=b kR}|%{= kw}%+w%{G}]"


